@model  IEnumerable<DataApp.Models.Request>

@{
    ViewData["Title"] = "List";
}

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Adı</th>
            <th>Email</th>
            <th>Telefon</th>
            <th>Mesaj</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.EMail</td>
                <td>@item.Phone</td>
                <td>@item.Message</td>
            </tr>
        }

    </tbody>
</table>

